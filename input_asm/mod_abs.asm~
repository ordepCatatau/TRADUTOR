				SECTION	TEXT
				MOD_ABS:	BEGIN
				PUBLIC	MOD_ABS
				PUBLIC	SAIDA_ABS
				VOLTA:	EXTERN
				INPUT	ENTRADA_ABS
				LOAD	ENTRADA_ABS
				JMPP	FIM_ABS
				JMPZ	FIM_ABS
CASO_NEGATIVO:	OUTPUT	MINUS_ONE
FIM_ABS:		STORE SAIDA_ABS
				JMP 	VOLTA
				STOP

SECTION	DATA
MINUS_ONE:	CONST -1
ENTRADA_ABS:	SPACE
SAIDA_ABS:	SPACE
END
